echo "Inizio..."

echo "Concorrenza 1 statico senza keepalive..."
sudo /usr/sbin/apachectl restart
echo "Riavviato..."
sleep 30
ab -n 10000 -c 1 http://127.0.0.1:80/statico.html
sudo /usr/sbin/apachectl restart
echo "Riavviato..."
sleep 30
ab -n 10000 -c 1 http://127.0.0.1:80/statico.html
sudo /usr/sbin/apachectl restart
echo "Riavviato..."
sleep 30
ab -n 10000 -c 1 http://127.0.0.1:80/statico.html

echo "Concorrenza 1 statico con keepalive..."
sudo /usr/sbin/apachectl restart
echo "Riavviato..."
sleep 30
ab -n 10000 -c 1 -k http://127.0.0.1:80/statico.html
sudo /usr/sbin/apachectl restart
echo "Riavviato..."
sleep 30
ab -n 10000 -c 1 -k http://127.0.0.1:80/statico.html
